#include <stdio.h>
int main() {
    int i, j, k, n, f, pages[50], frame[10], count = 0, flag1, flag2, pos, temp[10], max;
    printf("Enter number of pages: ");
    scanf("%d", &n);
    printf("Enter reference string: ");
    for (i = 0; i < n; i++) scanf("%d", &pages[i]);
    printf("Enter number of frames: ");
    scanf("%d", &f);
    for (i = 0; i < f; i++) frame[i] = -1;
    printf("Page\tFrames\n");
    for (i = 0; i < n; i++) {
        flag1 = flag2 = 0;
        for (j = 0; j < f; j++)
            if (frame[j] == pages[i]) {
                flag1 = flag2 = 1;
                break;
            }
        if (flag1 == 0)
            for (j = 0; j < f; j++)
                if (frame[j] == -1) {
                    count++;
                    frame[j] = pages[i];
                    flag2 = 1;
                    break;
                }
        if (flag2 == 0) {
            for (j = 0; j < f; j++) {
                temp[j] = -1;
                for (k = i + 1; k < n; k++)
                    if (frame[j] == pages[k]) {
                        temp[j] = k;
                        break;
                    }
            }
            pos = 0;
            for (j = 1; j < f; j++)
                if (temp[j] == -1) {
                    pos = j;
                    break;
                } else if (temp[j] > temp[pos]) pos = j;
            frame[pos] = pages[i];
            count++;
        }
        printf("%d\t", pages[i]);
        for (j = 0; j < f; j++) printf("%d ", frame[j]);
        printf("\n");
    }
    printf("Page Faults = %d\n", count);
}
